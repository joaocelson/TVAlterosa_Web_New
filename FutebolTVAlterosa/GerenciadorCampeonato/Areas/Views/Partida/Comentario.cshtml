@model IEnumerable<Campeonato.Dominio.JogoOnline>
    <!--  Campeonato.Dominio.JogoOnline> -->
    @{
        ViewBag.Title = "Futebol Lima Duarte";
    }

    @using (Html.BeginForm())
    {  @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)


        <div>

            <table>
                @if (Model.Count() > 0)
                {
                    <div>
                        Tempo
                    </div>
                    <div>

                    </div>
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.DataHoraComentario) -- 
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.Comentario)
                            </td>

                        </tr>

                    }

                }
                else
                {
                    <h1>Partida não possuí comentário.</h1>
                }
        </div>
    }
    </table>
